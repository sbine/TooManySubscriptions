{
    "name": "Too Many Subscriptions",
    "version": "0.0.1",
    "author": "Sara Bine <sara@sarabine.com>",
    "description": "Manage YouTube subscriptions.",
    "permissions": [
        "tabs",
        "*://*.youtube.com/*",
        "*://youtube.com/*"
    ],
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Too Many Subscriptions",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
